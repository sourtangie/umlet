<?xml version="1.0" encoding="UTF-8"?>
<!-- When updating your version of GWT, you should also update this DTD reference, so that your app can take advantage of the latest GWT module capabilities. -->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module>
    <inherits name='baseletgwt' />
    <inherits name="com.Web" />

    <replace-with class="com.web.gwt.client.view.utils.WebStartupDiagramLoader">
        <when-type-is class="com.baselet.gwt.client.view.utils.StartupDiagramLoader"/>
    </replace-with>

    <replace-with class="com.web.gwt.client.clipboard.LocalStorageClipboard">
        <when-type-is class="com.baselet.gwt.client.clipboard.ClipboardStorage"/>
    </replace-with>

    <replace-with class="com.web.gwt.client.clipboard.WebClipboardShortcutWrapper">
        <when-type-is class="com.baselet.gwt.client.clipboard.ClipboardShortcutWrapper"/>
    </replace-with>

    <replace-with class="com.web.gwt.client.view.widgets.WebDownloadPopupPanel">
        <when-type-is class="com.baselet.gwt.client.view.widgets.DownloadPopupPanel"/>
    </replace-with>

    <replace-with class="com.web.gwt.client.file.WebFileChangeNotifier">
        <when-type-is class="com.baselet.gwt.client.file.FileChangeNotifier"/>
    </replace-with>

    <replace-with class="com.web.gwt.client.view.theme.ThemeFactoryWeb">
        <when-type-is class="com.baselet.diagram.draw.helper.theme.ThemeFactory"/>
    </replace-with>
</module>
